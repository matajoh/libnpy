# Config file for the libnpy package.
# It defines the following variables:
# - LIBNPY_INCLUDE_DIRS: Include directories.
# - LIBNPY_LIBS: Libraries to link against.

get_filename_component (_BIN_DIR "${CMAKE_CURRENT_LIST_DIR}/../build/native" ABSOLUTE)
set(LIBNPY_INCLUDE_DIRS "${_BIN_DIR}/include")
set(LIBNPY_LIBS "${_BIN_DIR}/lib/@LIBNPY_NUGET_LIB@")

# Unset private variables.
unset (_BIN_DIR)
